{
  "name": "joe-pritchard/laravel-twitter-text-formatter",
  "description": "A wrapper package around a text formatter which takes output from twitter's API and returns an HTML string of the full tweet",
  "type": "library",
  "keywords": ["Twitter", "Parser", "Formatter", "HTML", "laravel-5-package"],
  "homepage": "https://github.com/joe-pritchard/laravel-twitter-text-formatter",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Joe Pritchard",
      "email": "joe@joe-pritchard.uk",
      "homepage": "https://joe-pritchard.uk"
    }
  ],
  "support": {
    "issues": "https://bitbucker.org/joe-pritchard/laravel-twitter-text-formatter/issues",
    "source": "https://bitbucket.org/joe-pritchard/laravel-twitter-text-formatter"
  },
  "require": {
    "php": "^7.0",
    "laravel/framework": "^5.0"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {"JoePritchard\\LaravelTwitterTextFormatter\\": "src/"},
    "files": ["helper.php"]
  },
  "extra": {
    "laravel": {
      "providers": [
        "JoePritchard\\LaravelTwitterTextFormatter\\Providers\\ServiceProvider"
      ]
    }
  }
}

